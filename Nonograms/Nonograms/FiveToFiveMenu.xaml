<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Nonograms.FiveToFiveMenu"
             Title="5x5">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="FillAndExpand">
            <Frame BorderColor="Gray" BackgroundColor="#e1e1e1" CornerRadius="15" Padding="0">
                <Button x:Name="SnakeButton" BackgroundColor="Transparent" Text="Змейка" FontSize="Large" HorizontalOptions="Center" TextColor="Black" Clicked="SnakeButton_Clicked"/>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>